{
  "name": "pk-icons",
  "version": "1.0.0",
  "type": "module",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "types": "./src/index.d.ts",
  "files": ["lib", "src"],
  "scripts": {
    "build": "vite build --config vite.config.lib.js",
