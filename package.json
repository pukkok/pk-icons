  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@vitejs/plugin-react": "^4.5.1",
    "gh-pages": "^6.3.0",
    "vite": "^6.3.5"
  }
}
